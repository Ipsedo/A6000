struct complex (
  integer i;
  integer j;
)

struct mega_complex extends complex (
  integer k;
)

struct super_complex (
  complex s_i;
  complex s_j;
)

main(integer x) (
  var complex a;
  var super_complex b;
  var mega_complex c;
  var []integer i;

  a := new complex();
  a.i := x;
  a.j := x + 1;

  print(a.i);
  print(a.j);
  
  b := make_super_complex(a);

  print(10);
  print(b.s_i.i);
  print(b.s_i.j);
  print(b.s_j.i);
  print(b.s_j.j);

  c := new mega_complex();
  c.i := x;
  c.j := c.i + 1;
  c.k := c.j + 1;

  print(c.i);
  print(c.j);
  print(c.k);
)

super_complex make_super_complex(complex a) (
  var super_complex b;

  b := new super_complex();
  b.s_i := a;

  b.s_j := new complex();
  b.s_j.i := b.s_i.j + 1;
  b.s_j.j := b.s_j.i + 1;

  result := b;
)
