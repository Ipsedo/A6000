
main(integer x) (
  var integer current_it;
  var integer i;
  var integer j;
  var integer c_r;
  var integer c_i;
  var integer z_r;
  var integer z_i;
  var integer tmp;

  current_it := 0;

  for i := 0; i < 50; i++ (
    for j := 0; j < 50; j++ (
      c_r := i - 50 / 2;
      c_i := j - 50 / 2;
      z_r := 0;
      z_i :=0;
      current_it := 0;

      while z_r*z_r + z_i*z_i < 4 && current_it < 50 (
        tmp := z_r;
        z_r := z_r*z_r - z_i*z_i + c_r;
        z_i := 2*z_i*tmp + c_i;
        current_it++;
      );

      if current_it == 50 then (
        print(35);
        print(32);
      ) else (
        print(46);
        print(32);
      );

    );
    print(10);
  );

)

