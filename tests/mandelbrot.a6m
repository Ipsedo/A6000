main(integer x) (
  var integer iteration_max;
  var integer current_it;
  var integer i;
  var integer j;
  var integer c_r;
  var integer c_i;
  var integer z_r;
  var integer z_i;
  var integer tmp;

  iteration_max := 50;
  current_it := 0;

  for i := 0; i < x; i++ (
    for j := 0; j < x; j++ (
      c_r := i - x / 2;
      c_i := j - x / 2;
      z_r := 0;
      z_i :=0;
      current_it := 0;

      while z_r*z_r + z_i*z_i < 4 && current_it < iteration_max (
        tmp := z_r;
        z_r := z_r*z_r - z_i*z_i + c_r;
        z_i := 2*z_i*tmp + c_i;
        current_it++;
      );

      if current_it == iteration_max then (
        print(35);
        print(32);
      ) else (
        print(46);
        print(32);
      );

    );
    print(10);
  );

)
